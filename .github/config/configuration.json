{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "feat"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix:"
      ]
    },
    {
      "title": "🧰 Maintenance",
      "labels": [
        "chore:"
      ]
    },
    {
      "title": "## 🧪 Tests",
      "labels": [
        "test:"
      ]
    },
    {
      "title": "## 🖍️ Documentation",
      "labels": [
        "doc:"
      ]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": [
        "dependencies:"
      ]
    }
  ],
  "sort": "ASC",
  "pr_template": "${{TITLE}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.+): (.+)",
      "target": "$1"
    }
  ],
  "exclude_merge_branches": [
    "merge pull request",
    "Merge pull request"
  ]
}