{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "feat"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix"
      ]
    },
    {
      "title": "🧰 Maintenance",
      "labels": [
        "chore"
      ]
    },
    {
      "title": "## 🧪 Tests",
      "labels": [
        "test"
      ]
    },
    {
      "title": "## 🖍️ Documentation",
      "labels": [
        "doc"
      ]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": [
        "dependencies"
      ]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "${{TITLE}}",
  "empty_template": "- no changes",
  "tag_resolver": {
    "method": "sort"
  },
  "exclude_merge_branches": [
    "merge pull request",
    "Merge pull request"
  ]
}